{
  "name": "leanseed",
  "version": "0.2.0",
  "description": "a leanCloud seed project based on Express and Vue.js",
  "main": "build/index.js",
  "scripts": {
    "start": "node build/index.js",
    "debug": "DEBUG=leanseed,leanseed:* node app.js",
    "build-server": "node_modules/.bin/babel server -d build",
    "build-app": "NODE_ENV=production node_modules/.bin/webpack -p",
    "build": "npm run build-server && npm run build-app",
    "predeploy": "npm run build",
    "deploy": "lean deploy",
    "pub": "lean publish",
    "dev": "node_modules/.bin/webpack-dev-server --inline --hot --port 8080",
    "init-hotel": "node_modules/.bin/webpack-dev-server --inline --hot --port $PORT --host 0.0.0.0",
    "hotel": "hotel add 'npm run init-hotel' -n 'leanseed'"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "keywords": [
    "leancloud",
    "avoscloud",
    "LeanCloud",
    "vue.js",
    "vue"
  ],
  "author": "turing <o.u.turing@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "express": "4.13.1",
    "leanengine": "^1.0.0-beta",
    "body-parser": "^1.13.2",
    "cookie-parser": "^1.3.5"
  },
  "devDependencies": {
    "babel-cli": "^6.0.0",
    "babel-core": "^6.4.0",
    "babel-loader": "^6.2.1",
    "babel-plugin-transform-runtime": "^6.4.3",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.4.3",
    "babel-runtime": "^5.8.34",
    "css-loader": "^0.23.1",
    "debug": "^2.2.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.17.0",
    "normalize.css": "^3.0.3",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-cssnext": "^2.5.2",
    "postcss-import": "^8.1.2",
    "postcss-loader": "^0.9.1",
    "postcss-reporter": "^1.3.3",
    "postcss-url": "^5.1.2",
    "precss": "^1.4.0",
    "style-loader": "^0.13.0",
    "uglify-js": "^2.4.23",
    "uglify-loader": "^1.2.0",
    "url-loader": "^0.5.7",
    "vue": "1.0.14",
    "vue-hot-reload-api": "^1.2.2",
    "vue-html-loader": "^1.1.0",
    "vue-loader": "^8.0.2",
    "vue-router": "^0.7.9",
    "vue-style-loader": "^1.0.0",
    "webpack": "^1.12.11",
    "webpack-cleanup-plugin": "^0.2.0",
    "webpack-dev-server": "^1.14.1"
  }
}
